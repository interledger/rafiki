    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "pino-pretty": "^11.0.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17"
