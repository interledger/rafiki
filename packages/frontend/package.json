    "@remix-run/react": "^2.15.3",
    "@remix-run/serve": "^2.15.3",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "graphql": "^16.8.1",
    "ilp-packet": "3.1.4-alpha.2",
    "isbot": "^5.1.22",
